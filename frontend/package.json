{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "BACKEND_URL=http://localhost:5045 next dev",
    "dev:docker": "BACKEND_URL=http://localhost:8080 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "test:e2e:headed": "cypress run --headed --spec 'cypress/e2e/**/*.cy.js'",
    "test:e2e:chrome": "cypress run --browser chrome --spec 'cypress/e2e/**/*.cy.js'",
    "test:e2e:firefox": "cypress run --browser firefox --spec 'cypress/e2e/**/*.cy.js'",
    "test:auth": "cypress run --spec 'cypress/e2e/auth/*.cy.js'",
    "test:orders": "cypress run --spec 'cypress/e2e/orders/*.cy.js'",
    "test:simulations": "cypress run --spec 'cypress/e2e/simulations/*.cy.js'",
    "test:dashboard": "cypress run --spec 'cypress/e2e/dashboard/*.cy.js'",
    "test:integration": "cypress run --spec 'cypress/e2e/integration/*.cy.js'",
    "test:component": "cypress run --component",
    "test:all": "npm run test:component && npm run test:e2e",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "lucide-react": "^0.513.0",
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "cypress": "^14.5.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8"
  }
}
